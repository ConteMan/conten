{
  "name": "contea",
  "productName": "Contea",
  "version": "0.3.7",
  "private": true,
  "description": "Personal Desktop Tool",
  "author": {
    "name": "ConteMan",
    "email": "real.conteman@gmail.com",
    "url": "https://isconte.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ConteMan/contea-desktop",
  "repository": {
    "type": "github",
    "url": "https://github.com/ConteMan/contea-desktop"
  },
  "keywords": [
    "electron",
    "tool"
  ],
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@koa/router": "^12.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "lodash": "^4.17.21",
    "sequelize": "^6.31.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/mdi": "^1.1.50",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/koa": "^2.13.6",
    "@types/koa__router": "^12.0.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "18.15.11",
    "@types/sqlite3": "^3.1.8",
    "@unocss/preset-attributify": "^0.51.2",
    "@unocss/preset-icons": "^0.51.2",
    "@unocss/reset": "^0.51.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^9.13.0",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.1",
    "electron-vite": "^1.0.21",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^9.10.0",
    "less": "^4.1.3",
    "pinia": "^2.0.34",
    "pinia-plugin-persistedstate": "^3.1.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.2",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  }
}
